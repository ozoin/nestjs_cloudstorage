{
    "version": 1,
    "builds": [
      {
        "src": "dist/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.ts",
        "methods":[
            "GET",
            "POST",
            "DELETE"
        ]
      }
    ]
  }